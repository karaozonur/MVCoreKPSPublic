@model MVCoreKPSPublic.Models.ViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Home Page";
}


<form method="post" asp-controller="Home" asp-action="Index">
    <div class="form-group">
        <label for="exampleInputEmail1">TC NO</label>
        @Html.TextBoxFor(model => model._parametters.TCKimlikNo,  new { @class = "form-control", @type="number",id="TCKimlikNo" } )
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">adı</label>
        @Html.TextBoxFor(model => model._parametters.Ad,  new { @class = "form-control", @type="text",id="Ad" } )
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Soyadı</label>
        @Html.TextBoxFor(model => model._parametters.Soyad,  new { @class = "form-control", @type="text",id="Soyadı" } )
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Dogum tarihi</label>
        @Html.TextBoxFor(model => model._parametters.DogumYili,  new { @class = "form-control", @type="text",id="DogumYili" } )
    </div>
    <button type="submit" class="btn btn-primary" asp-controller="Home" asp-action="Index">Kontrol Et</button>
</form>